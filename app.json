{
  "expo": {
    "name": "Golf Meaux-Boutigny",
    "slug": "NetGolf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.testntgol.NetGolf",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application utilise la cam√©ra pour scanner des QR codes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.testntgol.NetGolf",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "22d2839f-30a6-4723-be16-ce42b9daddca"
      }
    }
  }
}